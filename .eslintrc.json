{
    "env": {
      "browser": true,
      "commonjs": true,
      "es2020": true,
      "node": true
    },
    "extends": ["plugin:react/recommended", "airbnb", "plugin:prettier/recommended"],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12
    },
    "plugins": ["react"],
    "rules": {
      "react-hooks/exhaustive-deps" : "off",
      "no-console" : "off"
    }
  }